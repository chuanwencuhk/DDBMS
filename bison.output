Grammar

    0 $accept: sql_start $end

    1 sql_start: sql_func
    2          | EXIT

    3 sql_func: ';'
    4         | table_def
    5         | table_drop
    6         | insert_stat
    7         | delete_stat
    8         | select_stat

    9 table_def: CREATE TABLE table '(' table_attr_list ')' ';'

   10 table: NAME

   11 table_attr_list: column_def
   12                | table_attr_list ',' column_def

   13 column_def: column data_type

   14 column: NAME

   15 data_type: CHARACTER '(' NUMBER ')'
   16          | VARCHAR '(' NUMBER ')'
   17          | INTEGER
   18          | DATE

   19 table_drop: DROP TABLE table ';'

   20 insert_stat: INSERT INTO table VALUES '(' insert_list ')' ';'

   21 insert_list: NUMBER
   22            | STRING
   23            | insert_list ',' NUMBER
   24            | insert_list ',' STRING

   25 delete_stat: DELETE FROM table where_clause ';'

   26 select_stat: select_seg ';'

   27 select_seg: select_clause FROM fromlist where_clause

   28 select_clause: selectbegin unique sellist
   29              | selectbegin unique '*'

   30 selectbegin: SELECT

   31 unique: %empty
   32       | ALL
   33       | DISTINCT
   34       | UNIQUE

   35 sellist: sel_column
   36        | sellist ',' sel_column

   37 sel_column: NAME
   38           | NAME '.' NAME

   39 fromlist: sel_table
   40         | fromlist ',' sel_table

   41 sel_table: NAME
   42          | '(' select_seg ')'
   43          | '(' select_seg ')' NAME

   44 where_clause: %empty
   45             | WHERE condition

   46 condition: expr
   47          | condition AND expr

   48 expr: NAME '.' NAME COMPARISION NAME '.' NAME
   49     | NAME COMPARISION NUMBER
   50     | NAME COMPARISION STRING
   51     | NAME '.' NAME COMPARISION NUMBER
   52     | NAME '.' NAME COMPARISION STRING


